{
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "cSpell.words": [
    ],
    "cSpell.ignorePaths": [
        "**/package.json",
        "**/package-lock.json",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/objects/**",
        ".vscode",
        "yarn.lock",
        "jest.config.js",
        ".devcontainer",
        ".eslintrc.js"
    ]
}